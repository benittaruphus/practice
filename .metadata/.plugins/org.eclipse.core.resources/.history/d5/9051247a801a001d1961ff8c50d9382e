package com.ben.practice;

public class LargestPrimeFactor {

	
	int findLargestPrimeFactor(int n)
	{
		int m=n;
		while(n%2==0)
		{
			n /= 2;
		}
		
		for(int i=3; i<=n; i+=2)
		{
			if(n%i==0)
			{
				n /= i;
				m=i;
			}
		}
		return m;
	}
	
	public static void main (String[] arg)
	{
		LargestPrimeFactor l = new LargestPrimeFactor();
		System.out.println(l.findLargestPrimeFactor(100));
	}
}
