package com.ben.practice;

public class FrogRiverOne {
	public static void main(String[] args)
	{
		Solution s = new Solution();
		int[] A = {3, 1};
		int missing = s.solution(1, A);
		System.out.println(missing) ;
	}
}

class Solution {
    public int solution(int X, int[] A) {
    	
    	int l = A.length;
        if(l<X)
        {
            return -1;
        }
        if(sum(X) == sum(l))
        {
            return X;
        }
        
        for(int i:A)
        {
            
        }
        return 0;
    }
    
    int sum(int x)
    {
        return (x*(x+1))/2;
    }
}
