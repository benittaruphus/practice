package com.ben.practice;

public class TapeEquilibrium {
	public static void main(String[] args)
	{
		Solution s = new Solution();
		int[] A = {4};
		int missing = s.solution(A);
		System.out.println(missing) ;
	}
}

class Solution {
    public int solution(int[] A) {
    
    	int aL = A.length;
        int smallest = 0;
        
        if(aL==0)
        {
            return 0;
        }
        if(aL==1)
        {
            return A[0];
        }
        int[] p = new int[aL-1];
        Map<int, int> tapeParts = new HashMap<>();
        int firstHalf = 0;
        int secondHalf = 0;
        for(int i:A)
        {
        	firstHalf = A[i];
        }
        
        return smallest;
    }
    
    int sum(int n)
    {
        int sum = (n*(n+1))/2;
        return sum;
    }
}

